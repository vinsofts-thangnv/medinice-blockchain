    <script>
        $("a").on('click', function (event) {
            if (this.hash !== "") {
                event.preventDefault();
                var hash = this.hash;
                $('html, body').animate({
                    scrollTop: $(hash).offset().top
                }, 500, function () {
                    window.location.hash = hash;
                });
            }
        });

        var targetOffset = $("#nameProvider").offset().top;

        var $w = $(window).scroll(function () {
            var x= targetOffset-$w.scrollTop();
            if ($w.scrollTop() > targetOffset) {
                $('.navbar-brand').css({
                    margin: "0"
                });
            } else {
                $('.navbar-brand').css({
                    margin: x+"px auto 0"
                });
            }
        });

        $('#tableProducts').DataTable();
        $('#tableLicenses').DataTable();
    </script>
